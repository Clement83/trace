{
  "name": "guiv2-server",
  "version": "0.1.0",
  "private": true,
  "description": "API server for guiv2 - starts encode jobs and exposes SSE progress",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "install:types": "npm i --save-dev @types/node @types/express @types/cors @types/multer",
    "postinstall": "echo 'Tip: if TypeScript type packages are missing run npm run install:types in guiv2/server'"
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@types/fluent-ffmpeg": "^2.1.28",
    "canvas": "^3.2.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fast-xml-parser": "^4.2.4",
    "fluent-ffmpeg": "^2.1.3",
    "guiv2": "file:..",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.19.27",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "license": "MIT"
}
